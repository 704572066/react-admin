generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model users {
  id String  @id @map("_id")
  status String
  username  String
  password  String
  avatar  String
  balance Int
  promotionRate Int
  timezone  String
  createTime  DateTime
  version String?  @map("_v")
  openaiAccount String?
}