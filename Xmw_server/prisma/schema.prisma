generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model users {
  id String  @id @map("_id")
  status String
  username  String
  password  String
  avatar  String
  balance Int
  promotionRate Int
  timezone  String
  createTime  DateTime
  version Int?  @map("__v")
  openaiAccount String?
}

model apps {
  id String  @id @map("_id")
  name String
  avatar String
  permission String
  intro String?
  updateTime DateTime
  version Int?  @map("__v")
}

model datasets {
  id String  @id @map("_id")
  parentId String?
  teamId String
  tmbId String
  type String
  status String
  avatar String
  name String
  vectorModel String
  agentModel String
  intro String?
  updateTime DateTime
  permission String
  version Int?  @map("__v")
}